    <UserName>$PatrickChatService</UserName>
    <_SavePWD>True</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="ChatService.ChatContext" Order="1" Enabled="False">
