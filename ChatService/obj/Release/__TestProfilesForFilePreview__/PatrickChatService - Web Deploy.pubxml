          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="Server=tcp:avans-eu.database.windows.net,1433;Database=ChatDb;User ID=linksonder@avans-eu;Password=Abc12345;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;;" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
