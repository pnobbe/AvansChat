            <Source Path="DBContext" DbContext="ChatService.ChatContext, ChatService" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="ChatDB" Order="2" Enabled="True">
          <Destination Path="ov4qod7hzn.database.windows.net,1433" />
          <Object Type="DbDacFx">
            <PreSource Path="Data Source=tcp:avans-eu.database.windows.net,1433;Initial Catalog=ChatDb;User ID=linksonder@avans-eu;Password=Abc12345;Connect Timeout=30;Encrypt=True;TrustServerCertificate=False" includeData="False" />
